    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.0.0"
  },
  "scripts": {
    "start": "vite --force",
    "build": "vite build",
    "build:icons": "node scripts/generate-icons.js",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:coverage": "playwright test --reporter=html --reporter=json",
    "test:ci": "TZ=Europe/Berlin playwright test",
    "test:unit": "vitest run --coverage; node ../utility/patch-junit-xml.js test-results/junit-report.xml; pnpm dlx junit-to-ctrf -o ctrf/client-unit-ctrf.json test-results/junit-report.xml; node ../utility/patch-ctrf.js ctrf/client-unit-ctrf.json",
    "ci": "pnpm run build && pnpm run coverage",
    "coverage": "pnpm run coverage:clean && pnpm run test:unit && pnpm run coverage:copy-unit && pnpm run test:ci && pnpm run coverage:merge",
    "coverage:clean": "rm -rf .coverage-e2e coverage coverage-merged",
    "coverage:copy-unit": "mkdir -p .coverage-e2e && cp coverage/coverage-final.json .coverage-e2e/unit-coverage.json",
    "coverage:merge": "node scripts/merge-coverage.mjs",
    "coverage:report": "echo 'Run coverage:merge to generate report'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint": "^9.6.1",
    "@types/jsdom": "^27",
    "@types/opentype.js": "^1",
    "@types/react-router-dom": "latest",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-istanbul": "^4.0.18",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-i18next": "^6.1.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.2.0",
    "i18next-parser": "9.3.0",
    "i18next-scanner": "4.6.0",
    "jsdom": "^27.2.0",
    "monocart-coverage-reports": "^2.12.9",
    "opentype.js": "^1.3.4",
    "playwright-ctrf-json-reporter": "^0.0.27",
    "sharp-cli": "^5.2.0",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-plugin-istanbul": "^7.2.1",
    "vitest": "^4.0.18",
    "wait-on": "^9.0.3"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "node ../utility/set-version.js ${nextRelease.version}"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "execCwd": "..",
          "prepareCmd": "node utility/patch-workspace-versions.js && node utility/sync-root-version.js ${nextRelease.type}"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "execCwd": ".",
          "prepareCmd": "node ../utility/update-kustomize.js client frontend ${nextRelease.version}"
        }
      ],
      "@semantic-release/github",
      [
        "@codedependant/semantic-release-docker",
        {
          "dockerImage": "frontend",
          "dockerRegistry": "ghcr.io",
          "dockerProject": "fhswf/appointme",
          "dockerContext": ".."
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md",
            "k8s/overlays/**/kustomization.yaml"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
